                                            <option value="4">Bachelor of Commerce (Accounting) - summary</option>
                                        </select>
                                        <button class="form-submit btn btn-blue" id="submit-program-id">
                                            <span>Show Summary
                                                <i class="fa fa-search"></i>
                                            </span>
                                        </button>
                                        <div class="clearfix"></div>
                                    </form>
                                </div>

                                
                            </div>
                        </div>
                    </div>

